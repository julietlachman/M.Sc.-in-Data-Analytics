#SUMMARY
customers <- read_csv("/Users/Administrator/Downloads/MIS500M8PPOpt1.csv")
summary(customers)

customerschurn <- read_csv("/Users/Administrator/Downloads/MIS500M8PPOpt1sdf.csv")
