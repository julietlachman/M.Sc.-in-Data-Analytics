barplot(counts, main="Accounts for Seniors", xlab = "", ylab = "Customers:  7043",
names.arg=c("Non-Seniors: 5901", "Seniors: 1142"))
transform(as.data.frame(table(senior$SeniorCitizen == 1)),percentage=Freq/nrow(senior)*100)

